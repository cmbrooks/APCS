public class CompareSoundex extends AmericanSoundex implements Comparable<SortSoundex> {
	
	public int compareTo(String obj) {
		int result;
		
		if (obj > SortSoundex) {
			result = -1;
		} else if ( obj == CompareSoundex) {
			result = 0;
		} else {
			result = 1;
		}
		
		return result;
	}

	public String setSoundex (String x) {
		return AmericanSoundex.getSoundex(x);
	}

	public void printSoundex (String x) {
		System.out.println(setSoundex(x));
	}

}