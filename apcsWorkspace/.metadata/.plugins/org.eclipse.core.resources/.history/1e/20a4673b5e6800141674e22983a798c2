public class CompareSoundex extends AmericanSoundex implements Comparable {
	
	public int compareTo(Object obj1, Object obj2) {
		int result;
		if (obj1 -obj2 > 0) {
			result = -1;
		} else if (obj1 == obj2) {
			result = 0;
		} else if (obj1 <  obj2) {
			result = 1;
		}
		return result;
	}

	public void setSoundex (String x) {
		AmericanSoundex.getSoundex(x);
	}

	public void printSoundex (String x) {
		System.out.println(setSoundex(x));
	}

}