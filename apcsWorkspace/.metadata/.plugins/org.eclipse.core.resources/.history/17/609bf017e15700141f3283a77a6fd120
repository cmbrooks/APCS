
public class my_sqrt {

	public static void main(String[] args) {
		
		System.out.println(squareRoot(9));

	}
	
	public static double squareRoot(double d){
		double x0 = d/2;
		System.out.println(x0);
		double approx = x0;
		for(int i = 0; i < 5; i++){
			approx = (((x0+d)/x0)/2);
			System.out.println(approx);
		}
		return approx;
	}

}
